kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: jenkins
  namespace: 1546852608939-qa-jenkins
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: jenkins
        cbservice: jenkins
    spec:
      containers:
        - name: jenkins
          image: avarade/jenkins-java
          env:
            - name: CB_ENV_NAME
              value: 1546852608939-qa-jenkins
            - name: CB_CLUSTER_ID
              value: 5c330b050105780017ba3c65

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: jenkins
    cbservice: jenkins
  name: jenkins
  namespace: 1546852608939-qa-jenkins
spec:
  type: LoadBalancer
  ports:
    - port: 8080
      targetPort: 8080
      name: port8080
  selector:
    deploy: jenkins

