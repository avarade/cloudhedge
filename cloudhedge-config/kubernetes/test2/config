kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: wp-test2
  namespace: 1538552889565-test2-wp-test
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: wp-test2
        cbservice: wp-test2
    spec:
      containers:
        - name: wp-test2
          image: waghsagar1/docker-wp
          env:
            - name: CB_ENV_NAME
              value: 1538552889565-test2-wp-test
            - name: CB_CLUSTER_ID
              value: 5bb35231a99ef7000f2f5d1f

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: wp-test2
    cbservice: wp-test2
  name: wp-test2
  namespace: 1538552889565-test2-wp-test
spec:
  type: LoadBalancer
  ports:
    - port: 72
      targetPort: 72
      name: port72
  selector:
    deploy: wp-test2

