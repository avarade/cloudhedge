kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: jbpmconsole
  namespace: 1539174766544-v1-jbpm
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: jbpmconsole
        cbservice: jbpmconsole
    spec:
      containers:
        - name: jbpmconsole
          image: 'jboss/jbpm-workbench-showcase:latest'
          env:
            - name: CB_ENV_NAME
              value: 1539174766544-v1-jbpm
            - name: CB_CLUSTER_ID
              value: 5bbb6499321a38000e6da459

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: jbpmconsole
    cbservice: jbpmconsole
  name: jbpmconsole
  namespace: 1539174766544-v1-jbpm
spec:
  type: LoadBalancer
  ports:
    - port: 8080
      targetPort: 8080
      name: port8080
  selector:
    deploy: jbpmconsole

