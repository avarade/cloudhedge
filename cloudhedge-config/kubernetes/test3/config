kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: t3
  namespace: 1538566975791-test3-coltest-sw
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: t3
        cbservice: t3
    spec:
      containers:
        - name: t3
          image: gurunath/colortest
          env:
            - name: CB_ENV_NAME
              value: 1538566975791-test3-coltest-sw
            - name: CB_CLUSTER_ID
              value: 5bb35231a99ef7000f2f5d1f

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: t3
    cbservice: t3
  name: t3
  namespace: 1538566975791-test3-coltest-sw
spec:
  type: LoadBalancer
  ports:
    - port: 80
      targetPort: 80
      name: port80
  selector:
    deploy: t3

