kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: icetest
  namespace: 1538746743770-ice1-blist-sw
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: icetest
        cbservice: icetest
    spec:
      containers:
        - name: icetest
          image: amithaval/icescrum
          env:
            - name: CB_ENV_NAME
              value: 1538746743770-ice1-blist-sw
            - name: CB_CLUSTER_ID
              value: 5bb35231a99ef7000f2f5d1f

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: icetest
    cbservice: icetest
  name: icetest
  namespace: 1538746743770-ice1-blist-sw
spec:
  type: LoadBalancer
  ports:
    - port: 8080
      targetPort: 8080
      name: port8080
  selector:
    deploy: icetest

