kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: ice
  namespace: 1537784024197-test-icescrum
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: ice
        cbservice: ice
    spec:
      containers:
        - name: ice
          image: amithaval/ice_test2
          env:
            - name: CB_ENV_NAME
              value: 1537784024197-test-icescrum
            - name: CB_CLUSTER_ID
              value: 5ba8afeb6efcb30010dab636

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: ice
    cbservice: ice
  name: ice
  namespace: 1537784024197-test-icescrum
spec:
  type: LoadBalancer
  ports:
    - port: 8080
      targetPort: 8080
      name: port8080
  selector:
    deploy: ice

