kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: test
  namespace: 1537769574094-q12-icescrum
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: test
        cbservice: test
    spec:
      containers:
        - name: test
          image: amithaval/ice_test
          env:
            - name: CB_ENV_NAME
              value: 1537769574094-q12-icescrum
            - name: CB_CLUSTER_ID
              value: 5ba107b4e39495000e12c108

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: test
    cbservice: test
  name: test
  namespace: 1537769574094-q12-icescrum
spec:
  type: LoadBalancer
  ports:
    - port: 8080
      targetPort: 8080
      name: port8080
  selector:
    deploy: test

