kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: wp-test1
  namespace: 1538552210356-test1-wp-test
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: wp-test1
        cbservice: wp-test1
    spec:
      containers:
        - name: wp-test1
          image: waghsagar1/docker-wp3
          env:
            - name: CB_ENV_NAME
              value: 1538552210356-test1-wp-test
            - name: CB_CLUSTER_ID
              value: 5bb35231a99ef7000f2f5d1f

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: wp-test1
    cbservice: wp-test1
  name: wp-test1
  namespace: 1538552210356-test1-wp-test
spec:
  type: LoadBalancer
  ports:
    - port: 71
      targetPort: 71
      name: port71
  selector:
    deploy: wp-test1

