kind: Deployment
apiVersion: extensions/v1beta1
metadata:
  name: test
  namespace: 1537767360780-test123-icescrum
spec:
  replicas: 1
  template:
    metadata:
      labels:
        deploy: test
        cbservice: test
    spec:
      containers:
        - name: test
          image: amithaval/ice_test
          env:
            - name: CB_ENV_NAME
              value: 1537767360780-test123-icescrum
            - name: CB_CLUSTER_ID
              value: 5b4f1458f96b3a000e67a29d

---
kind: Service
apiVersion: v1
metadata:
  labels:
    service: test
    cbservice: test
  name: test
  namespace: 1537767360780-test123-icescrum
spec:
  type: LoadBalancer
  ports:
    - port: 8080
      targetPort: 8080
      name: port8080
  selector:
    deploy: test

